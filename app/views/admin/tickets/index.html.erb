<div class="container">
  <div class="wrapper">
    <ul>
      <% @tickets.each do |ticket| %>
        <div class="card">
          <ul class='list-unstyled list-inline'>
            <li><%= ticket.name_show %></li>
            <li><%= ticket.price %></li>
            <li><%= ticket.date %></li>
            <li><%= ticket.client %></li>
          </ul>
        </div>
      <% end %>
    </ul>
  </div>
</div>
