<h1>Admin::Actors#index</h1>
<p>Find me in app/views/admin/actors/index.html.erb</p>


<div class="container">
  <div class="wrapper">
    <ul>
      <% @actors.each do |actor| %>
        <div class="card">
          <ul>
            <li><%= actor.first_name %></li>
            <li><%= link_to 'show', admin_actor_path(actor) %></li>
            <li><%= link_to 'edit', edit_admin_actor_path(actor) %></li>
            <li><%= link_to 'delete', admin_actor_path(actor) %></li>
          </ul>
        </div>
      <% end %>
    </ul>
  </div>
</div>
